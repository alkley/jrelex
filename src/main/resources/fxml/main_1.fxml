<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.net.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<BorderPane prefHeight="800.0" prefWidth="1100.0" xmlns:fx="http://javafx.com/fxml" fx:controller="si.comptus.jrelex.MainController">
  <center>
    <TabPane id="tabPanePageShow" fx:id="tabPaneTopDisplay" />
  </center>
  <top>
    <MenuBar>
      <menus>
        <Menu mnemonicParsing="false" text="File">
          <items>
            <MenuItem mnemonicParsing="false" text="Explore database" fx:id="menuItemExploreDatabase" />
            <MenuItem mnemonicParsing="false" text="Close" fx:id="menuItemClose" />
          </items>
        </Menu>
        <Menu mnemonicParsing="false" text="Edit">
          <items>
            <MenuItem id="" mnemonicParsing="false" text="Manage databases" fx:id="menuItemManageDatabase" />
          </items>
        </Menu>
        <Menu mnemonicParsing="false" text="Help">
          <items>
            <MenuItem mnemonicParsing="false" text="About" />
          </items>
        </Menu>
      </menus>
    </MenuBar>
  </top>
</BorderPane>
